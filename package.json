{
  "name": "@acneidert/appwrite_offline",
  "version": "0.0.1",
  "description": "Appwrite Client with Offline Option",
  "keywords": [
    "Appwrite",
    "Offline",
    "Offline First"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Andre Neidert <acneidert@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/acneidert/appwrite_offline",
  "bugs": {
    "url": "https://github.com/acneidert/appwrite_offline/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acneidert/appwrite_offline.git"
  },
  "dependencies": {
    "appwrite": "^10.1.0",
    "minimongo": "^6.12.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "fake-indexeddb": "^4.0.0",
    "jest": "^29.1.2",
    "jest-localstorage-mock": "^2.4.22",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  }
}
